{
	"name": "ngx-view",
	"version": "1.0.0",
	"description": "Angular tests for view",
	"keywords": [
		"angular",
		"webpack",
		"karma",
		"jasmine",
		"starter"
	],
	"author": "Łukasz Pawełczak",
	"license": "MIT",
	"scripts": {
		"purge": "rimraf node_modules && rimraf dist",
		"clean": "rimraf dist",
		"clean:aot": "rimraf dist-aot",
		"clean:install": "yarn clean && rimraf node_modules && npm i",
		"prewebpack:build": "yarn clean",
		"webpack:build": "webpack --progress",
		"build": "yarn build:dev",
		"build:dev": "yarn webpack:build -- --config config/webpack.dev.js",
		"build:prod": "yarn webpack:build -- --config config/webpack.prod.js",
		"watch": "yarn webpack:build -- --config config/webpack.dev.js --watch",
		"server": "yarn server:dev",
		"server:dev": "webpack-dev-server --config config/webpack.dev.js --inline --progress --port 3000  --content-base src",
		"server:prod": "webpack-dev-server --config config/webpack.prod.js --inline --progress --port 3000  --content-base src",
		"start": "yarn server:dev",
		"start:prod": "yarn server:prod",
		"test": "karma start",
		"test:watch": "karma start --no-single-run --auto-watch",
		"ngc": "ngc -p tsconfig.aot.json --locale=en-US",
		"webpack:aot": "ngc -p tsconfig.aot.json --locale=en-US && webpack --config config/webpack.aot.js",
		"prewebpack:aot": "yarn clean:aot",
		"webdriver-manager": "webdriver-manager",
		"webdriver:update": "webdriver-manager update",
		"protractor": "protractor",
		"pree2e": "yarn webdriver:update -- --standalone",
		"e2e": "yarn protractor",
		"analyze-bundles": "yarn webpack:build -- --config config/webpack.dev.js --profile --json > dist/stats.json && webpack-bundle-analyzer dist/stats.json"
	},
	"dependencies": {
		"@angular/animations": "8.0.0",
		"@angular/common": "8.0.0",
		"@angular/compiler": "8.0.0",
		"@angular/core": "8.0.0",
		"@angular/forms": "8.0.0",
		"@angular/platform-browser": "8.0.0",
		"@angular/platform-browser-dynamic": "8.0.0",
		"@angular/platform-server": "8.0.0",
		"@angular/router": "8.0.0",
		"core-js": "^2.4.1",
		"es6-shim": "^0.35.1",
		"ie-shim": "^0.1.0",
		"jquery": "^3.5.0",
		"lodash": "^4.17.15",
		"reflect-metadata": "^0.1.3",
		"rxjs": "^6.4.0",
		"rxjs-compat": "^6.5.5",
		"zone.js": "^0.9.1"
	},
	"devDependencies": {
		"@angular/compiler-cli": "8.0.0",
		"@types/cucumber": "0.0.31",
		"@types/es6-shim": "0.0.31",
		"@types/jasmine": "^2.6.0",
		"@types/lodash": "ts2.0",
		"@types/node": "^6.0.38",
		"@types/selenium-webdriver": "^2.53.38",
		"angular2-template-loader": "^0.4.0",
		"autoprefixer": "^6.5.1",
		"awesome-typescript-loader": "~3.1.2",
		"bootstrap-loader": "^1.3.3",
		"chai": "^3.5.0",
		"chai-as-promised": "^6.0.0",
		"copy-webpack-plugin": "^3.0.1",
		"css-loader": "^0.23.1",
		"cucumber": "^1.3.0",
		"cucumber-tsflow": "^1.2.1",
		"exports-loader": "^0.6.3",
		"file-loader": "^0.9.0",
		"html-webpack-plugin": "^2.22.0",
		"husky": "^0.14.3",
		"imports-loader": "^0.7.0",
		"jasmine-core": "^2.4.3",
		"jasmine-marbles": "0.3.1",
		"karma": "^1.2.0",
		"karma-chrome-launcher": "^2.2.0",
		"karma-jasmine": "^1.0.2",
		"karma-sourcemap-loader": "^0.3.7",
		"karma-webpack": "2.0.1",
		"node-sass": "^3.10.1",
		"null-loader": "^0.1.1",
		"phantomjs-prebuilt": "^2.1.12",
		"precss": "^1.4.0",
		"protractor": "^7.0.0",
		"protractor-cucumber-framework": "^0.6.0",
		"raw-loader": "^0.5.1",
		"resolve-url-loader": "^1.6.1",
		"sass-loader": "^4.0.2",
		"style-loader": "^0.13.1",
		"to-string-loader": "^1.1.4",
		"ts-helpers": "^1.1.1",
		"ts-node": "^3.3.0",
		"tslib": "^1.7.1",
		"tslint": "^3.14.0",
		"tslint-loader": "^2.1.5",
		"typescript": "3.4.x",
		"url-loader": "^0.5.7",
		"webpack": "2.5.1",
		"webpack-bundle-analyzer": "^3.3.2",
		"webpack-dev-server": "2.4.5",
		"webpack-merge": "4.1.0"
	}
}
