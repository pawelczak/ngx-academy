{
    "name": "ngx-view",
    "version": "1.0.0",
    "description": "Angular tests for view",
    "keywords": [
        "angular",
        "webpack",
        "karma",
        "jasmine",
        "starter"
    ],
    "author": "Łukasz Pawełczak",
    "license": "MIT",
    "scripts": {
        "purge": "rimraf node_modules && rimraf dist",
        "clean": "rimraf dist",
        "clean:aot": "rimraf dist-aot",
        "clean:install": "npm run clean && rimraf node_modules && npm i",
        "prewebpack:build": "npm run clean",
        "webpack:build": "webpack --progress",
        "build": "npm run build:dev",
        "build:dev": "npm run webpack:build -- --config config/webpack.dev.js",
        "build:prod": "npm run webpack:build -- --config config/webpack.prod.js",
        "watch": "npm run build -- --watch",
        "server": "npm run server:dev",
        "server:dev": "webpack-dev-server --config config/webpack.dev.js --inline --progress --port 3000  --content-base src",
        "server:prod": "webpack-dev-server --config config/webpack.prod.js --inline --progress --port 3000  --content-base src",
        "start": "npm run server:dev",
        "start:prod": "npm run server:prod",
        "test": "karma start",
        "test:watch": "karma start --no-single-run --auto-watch",
        "ngc": "ngc -p tsconfig.aot.json --locale=en-US",
        "webpack:aot": "ngc -p tsconfig.aot.json --locale=en-US && webpack --config config/webpack.aot.js",
        "prewebpack:aot": "npm run clean:aot",
        "webdriver-manager": "webdriver-manager",
        "webdriver:update": "webdriver-manager update",
        "protractor": "protractor",
        "pree2e": "npm run webdriver:update -- --standalone",
        "e2e": "npm run protractor"
    },
    "dependencies": {
        "@angular/common": "4.4.6",
        "@angular/compiler": "4.4.6",
        "@angular/core": "4.4.6",
        "@angular/forms": "4.4.6",
        "@angular/http": "4.4.6",
        "@angular/platform-browser": "4.4.6",
        "@angular/platform-browser-dynamic": "4.4.6",
        "@angular/platform-server": "4.4.6",
        "@angular/router": "4.4.6",
        "bootstrap": "^3.3.7",
        "bootstrap-sass": "^3.3.7",
        "core-js": "^2.4.1",
        "es6-shim": "^0.35.1",
        "ie-shim": "^0.1.0",
        "jquery": "^1.7.4",

        "lodash": "^4.17.3",
        "reflect-metadata": "^0.1.3",
        "rxjs": "5.5.0",
        "zone.js": "^0.8.12"
    },
    "devDependencies": {
        "@angular/compiler-cli": "4.4.6",
        "@types/cucumber": "0.0.31",
        "@types/es6-shim": "0.0.31",
        "@types/jasmine": "2.2.22-alpha",
        "@types/node": "^6.0.38",
        "@types/selenium-webdriver": "^2.53.38",
        "angular2-template-loader": "^0.4.0",
        "autoprefixer": "^6.5.1",
        "awesome-typescript-loader": "^1.1.1",
        "bootstrap-loader": "^1.3.3",
        "chai": "^3.5.0",
        "chai-as-promised": "^6.0.0",
        "copy-webpack-plugin": "^3.0.1",
        "css-loader": "^0.23.1",
        "cucumber": "^1.3.0",
        "cucumber-tsflow": "^1.2.1",
        "exports-loader": "^0.6.3",
        "file-loader": "^0.9.0",
        "html-webpack-plugin": "^2.22.0",
        "imports-loader": "^0.7.0",
        "jasmine-core": "^2.4.3",
        "karma": "^1.2.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-jasmine": "^1.0.2",
        "karma-phantomjs-launcher": "^1.0.1",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-webpack": "2.0.1",
        "node-sass": "^3.10.1",
        "null-loader": "^0.1.1",
        "phantomjs-prebuilt": "^2.1.12",
        "precss": "^1.4.0",
        "protractor": "^4.0.9",
        "protractor-cucumber-framework": "^0.6.0",
        "raw-loader": "^0.5.1",
        "resolve-url-loader": "^1.6.1",
        "sass-loader": "^4.0.2",
        "style-loader": "^0.13.1",
        "to-string-loader": "^1.1.4",
        "ts-helpers": "^1.1.1",
        "ts-node": "^1.7.2",
        "tslib": "^1.7.1",
        "tslint": "^3.14.0",
        "tslint-loader": "^2.1.5",
        "typescript": "2.1.1",
        "url-loader": "^0.5.7",
        "webpack": "2.5.1",
        "webpack-dev-server": "2.4.5",
        "webpack-merge": "4.1.0"
    }
}
